<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_move_tree_receipt_picking" model="ir.ui.view">
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_tree_receipt_picking" />
        <field name="arch" type="xml">
            <field name="origin" position="after">
                <field name="claim_id" optional="show" readonly="1" />
            </field>
        </field>
    </record>

    <record id="view_move_tree" model="ir.ui.view">
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_tree" />
        <field name="arch" type="xml">
            <field name="reference" position="after">
                <field name="claim_id" optional="show" readonly="1" />
            </field>
        </field>
    </record>

    <record id="view_move_form" model="ir.ui.view">
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_form" />
        <field name="arch" type="xml">
            <field name="reference" position="after">
                <field name="claim_id" readonly="1" />
            </field>
        </field>
    </record>

    <record id="view_stock_move_operations" model="ir.ui.view">
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_stock_move_operations" />
        <field name="arch" type="xml">
            <field name="product_id" position="before">
                <field name="claim_id" readonly="1" />
            </field>
        </field>
    </record>

    <record id="view_move_search" model="ir.ui.view">
        <field name="model">stock.move</field>
        <field name="inherit_id" ref="stock.view_move_search" />
        <field name="arch" type="xml">
            <field name="product_id" position="after">
                <field name="claim_id" readonly="1" />
            </field>
            <filter name="groupby_picking_id" position="after">
                <filter
          string="Claim"
          name="groupby_claim"
          domain="[]"
          context="{'group_by': 'claim_id'}"
        />
            </filter>
        </field>
    </record>
</odoo>
